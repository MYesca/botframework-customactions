{
  "pbiOptions": {
    "authorityUri": "https://login.microsoftonline.com/organizations/",
    "tenantId": "",
    "clientId": "",
    "clientSecret": "",
    "scope": "https://analysis.windows.net/powerbi/api/.default",
    "workspaceId": "",
    "renderUrl": "",
    "browserWsEndpoint": "",
    "browserDelayMilis": ""
  },
  "customFunctions": [],
  "defaultLanguage": "en-us",
  "defaultLocale": "en-us",
  "importedLibraries": [],
  "languages": [
    "en-us"
  ],
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "luFeatures": {
    "enablePattern": true,
    "enableMLEntities": true,
    "enableListEntities": true,
    "enableCompositeEntities": true,
    "enablePrebuiltEntities": true,
    "enableRegexEntities": true,
    "enablePhraseLists": true
  },
  "luis": {
    "name": "MySolutionBot",
    "authoringEndpoint": "https://westus.api.cognitive.microsoft.com",
    "endpoint": "https://westus.api.cognitive.microsoft.com",
    "authoringRegion": "westus",
    "defaultLanguage": "en-us",
    "environment": "composer",
    "directVersionPublish": true
  },
  "MicrosoftAppId": "",
  "publishTargets": [],
  "qna": {
    "knowledgebaseid": "",
    "hostname": "",
    "qnaRegion": "westus"
  },
  "runtime": {
    "command": "dotnet run --project MySolutionBot.csproj",
    "customRuntime": true,
    "key": "adaptive-runtime-dotnet-webapp",
    "path": "../"
  },
  "runtimeSettings": {
    "components": [
      {
        "name": "MySolution.BotFramework.CustomActions.PBI.Client"
      }
    ],
    "features": {
      "showTyping": false,
      "useInspection": false,
      "removeRecipientMentions": false,
      "blobTranscript": {}
    },
    "telemetry": {
      "options": {
        "instrumentationKey": ""
      }
    },
    "skills": {
      "allowedCallers": []
    }
  },
  "skillConfiguration": {},
  "downsampling": {
    "maxImbalanceRatio": -1
  },
  "skill": {},
  "speech": {
    "voiceFontName": "en-US-AriaNeural",
    "fallbackToTextForSpeechIfEmpty": true
  },
  "skillHostEndpoint": "http://localhost:3980/api/skills"
